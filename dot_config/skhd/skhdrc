hyper - n : open -a /Applications/Cursor.app ~/Cloud/everywhere/notes
hyper - 1 : ~/.bin/spotify-ctl playpause
hyper - 2 : ~/.bin/spotify-ctl next
hyper - 3 : ~/.bin/spotify-ctl previous
hyper - 4 : ~/.bin/spotify-ctl muteunmute
hyper - 5 : ~/.bin/spotify-ctl volup
hyper - 6 : ~/.bin/spotify-ctl voldown
hyper - 7 : ~/.bin/nvd
hyper - 8 : ~/.bin/show-the-roro
# hyper - 9 is set in System Settings > Keyboard > Shortcuts > Services > Start Screen Saver. Remember to install the automator service from iCloud Drive.
